[include]
	path = .gitconfig.local
[core]
	excludesfile = ~/.gitignore
	editor = vim -c \"set fenc=utf-8\"
	ignorecase = false
	quotepath = false
[color]
	ui = auto
[fetch]
	prune = true
[pull]
	rebase = true
[push]
	default = current
[rebase]
	autosquash = true
[difftool]
	prompt = false
[mergetool]
	prompt = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	a = add
	b = branch
	c = commit
	d = diff
	f = fetch --all
	g = reflog
	l = log
	m = merge
	p = pull
	r = rebase
	s = stash
	t = tag
	w = show
	aa = commit --amend -a --no-edit
	bd = branch -D
	bl = branch -a
	cb = checkout -b
	cm = checkout origin/HEAD
	co = checkout
	cp = cherry-pick
	cx = checkout 'HEAD@{1}'
	df = difftool
	du = difftool @{upstream}
	fm = fetch origin master:master
	gg = grep --line-number --show-function --heading --break
	mm = merge origin/HEAD
	pf = push --force-with-lease
	re = reset --soft HEAD~
	rh = reset --hard
	ri = rebase -i
	rr = rebase origin/HEAD
	rs = reset --soft
	sl = stash list
	st = status
	su = submodule update
	pop = stash pop
	amend = commit --amend --no-edit
	shallow = clone --depth 1
	skip = update-index --skip-worktree
	patch = format-patch -1 HEAD
[advice]
	skippedCherryPicks = false
